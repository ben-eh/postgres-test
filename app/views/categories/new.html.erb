<%= link_to image_tag("star.svg", width: 50), root_path %>
<h1>Add New Category</h1>

<%= render 'form' %>

<h3>Existing Categories</h3>

<ul>
  <% @categories.each do |category| %>
    <li>
      <%= category.name %>
      <%= link_to "Edit", edit_category_path(category.id) %>
    </li>
  <% end %>
</ul>
